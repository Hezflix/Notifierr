{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "PlexNotifierr": "Data Source=PATH\\TO\\DB.db;"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Quartz": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp} [{Level:u3}] {{{SourceContext}}} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "bin/Logs/PlexNotifierr.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp} [{Level:u3}] {{{SourceContext}}} {{{MessageType}:{MessageId}}} {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Environment": "Dev"
    },
    "Enrich": [
      "FromLogContext",
      "WithActivity",
      "WithMachineName"
    ]
  },
  "Plex": {
    "Product": "PlexNotifierr",
    "DeviceName": "PlexNotifierr",
    "ClientId": "PlexNotifierr",
    "ServerUrl": "https://example.com",
    "AccessToken": "AA_AA"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "Port": 5672,
    "VirtualHost":"/"
  }
}
